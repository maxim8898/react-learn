{
  "name": "react-two",
  "version": "1.0.0",
  "description": "This is project for second home task.",
  "main": "index.jsx",
  "scripts": {
    "test": "jest --watchAll",
    "test:coverage": "jest --coverage",
    "build": "webpack",
    "dev-build": "set NODE_ENV=development&&webpack",
    "prod-build": "set NODE_ENV=production&&webpack",
    "dev": "webpack-dev-server --mode development"
  },
  "author": "Maksim Matuzka",
  "dependencies": {
    "css-loader": "^2.1.1",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.4",
    "react-redux": "^6.0.1",
    "react-router": "^4.4.0",
    "react-scripts": "^2.0.3",
    "redux": "^4.0.1",
    "style-loader": "^0.23.1",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-react": "^7.0.0",
    "axios": "^0.18.0",
    "babel-jest": "^24.7.1",
    "babel-loader": "^8.0.0-beta.0",
    "babel-preset-env": "1.6.1",
    "cross-fetch": "^3.0.2",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "enzyme-to-json": "^3.3.5",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.7.1",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^16.2.0",
    "react-thunk": "^1.0.0",
    "redux-thunk": "^2.3.0"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "dist",
      "__snapshots__"
    ],
    "setupFiles": [
      "./test/jestsetup.js"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|scss)$": "identity-obj-proxy"
    }
  }
}
